{
    "name": "blackends/laravel-facebook-pixel",
    "description": "Facebook Pixel integration for Laravel",
    "keywords": [
        "blackends",
        "facebook-pixel",
        "laravel"
    ],
    "homepage": "https://github.com/Zubair-Iftikhar/laravel-facebook-pixel",
    "license": "MIT",
    "authors": [
        {
            "name": "BlackEnds",
            "email": "blackends@gmail.com"
        },
        {
            "name": "Zubair Iftikhar",
            "email": "zu0072010@gmail.com",
            "homepage": "https://blackends.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "facebook/php-business-sdk": "^12.0",
        "illuminate/contracts": "^8.0",
        "spatie/laravel-package-tools": "^1.11"
    },
    "require-dev": {
        "nunomaduro/collision": "^v5.11.0",
        "nunomaduro/larastan": "^1.0.4",
        "orchestra/testbench": "^v6.25.1",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Combindma\\FacebookPixel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Combindma\\FacebookPixel\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint",
        "format-v": "vendor/bin/pint -v"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Combindma\\FacebookPixel\\FacebookPixelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
